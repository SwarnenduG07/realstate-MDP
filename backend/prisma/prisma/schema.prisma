generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int        @id @default(autoincrement())
  firstname  String
  lastname   String
  username   String @unique
  email      String @unique
  password   String
}

model Property {
   id          Int    @id @default(autoincrement())
   title       String
   description String
   price       Float
   localtion   String
   user_id     Int
   
  
}